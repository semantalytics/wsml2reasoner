wsmlVariant _"http://www.wsmo.org/wsml/wsml-syntax/wsml-core"
namespace { _"http://simple6#"
}

ontology simple_6

concept A
concept B
concept C
concept D
	
instance A1 memberOf A
	  	x hasValue a
	    y hasValue b
		
instance B1 memberOf B
		x hasValue a
	    y hasValue b
		
instance C1 memberOf C
		x hasValue k
	    y hasValue l
		
axiom S definedBy
	 ?a = ?b :- ?a[x hasValue ?x] and ?a[y hasValue ?y] and ?b[x hasValue ?z] and ?b[y hasValue ?w] and ?x = ?z and ?y = ?w.
	 p(?x) :- ?x memberOf B. 
