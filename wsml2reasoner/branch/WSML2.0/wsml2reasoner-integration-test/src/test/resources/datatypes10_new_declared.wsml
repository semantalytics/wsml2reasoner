wsmlVariant _"http://www.wsmo.org/wsml/wsml-syntax/wsml-flight"
namespace { _"http://example.com/datatypes10#",
	wsml _"http://www.wsmo.org/wsml/wsml-syntax#", 
	xsd   _"http://www.w3.org/2001/XMLSchema#",
	rdf  _"http://www.w3.org/1999/02/22-rdf-syntax-ns#"}

ontology DatatypesTestOntology10

// See SOA4All D3.1.4 Defining the features of the WSML-Rule v2.0 language
// new datatypes rdf:XMLLiteral, rdf:Text, xs:yearMonthDuration, xs:dayTimeDuration


concept anEntity
	aDuration ofType xsd#duration
	// RDF Datatypes
	aPlainLiteral ofType rdf#text
	aRDF_XMLLiteral ofType rdf#XMLLiteral
	// XML Schema Datatypes
	aYearMonthDuration ofType xsd#yearMonthDuration
	aDayTimeDuration ofType xsd#dayTimeDuration
	
instance anInstance memberOf anEntity
	aDuration hasValue xsd#duration(1,2,3,4,5,6.0)
	aPlainLiteral hasValue rdf#text("This should be a RDF Text", "en")
	aRDF_XMLLiteral hasValue rdf#XMLLiteral("<tag>xml literal</tag>", "en")
	aYearMonthDuration hasValue xsd#yearMonthDuration(2009,9)
	aDayTimeDuration hasValue xsd#dayTimeDuration(5, 3, 2, 1.0)
	


