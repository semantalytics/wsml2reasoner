wsmlVariant _"http://www.wsmo.org/wsml/wsml-syntax/wsml-core"
namespace { _"http://simple#"
}

ontology simple

concept C1
concept C2

instance a memberOf C1
	name hasValue aName

instance b memberOf C2
	
	
axiom equalInHead definedBy
	a = b :- wsml#true. 
	p(?x) :- ?x memberOf C2 and ?x[name hasValue aName].
