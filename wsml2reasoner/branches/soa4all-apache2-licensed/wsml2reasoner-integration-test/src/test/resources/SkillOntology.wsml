axiom direct_instance definedBy
	di(?x, ?T) :- ?x memberOf ?T and naf(ndi(?x, ?T)).
	ndi(?x, ?T) :- ?x memberOf ?T and ?subT subConceptOf ?T and ?x memberOf ?subT.
	