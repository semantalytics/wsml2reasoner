wsmlVariant _"http://www.wsmo.org/wsml/wsml-syntax/wsml-core"
namespace { _"http://simple1#"
}

ontology simple_1

concept X

concept Y

instance x1 memberOf X
     name hasValue Adrian
         
instance x2 memberOf Y
     name hasValue Adrian  

axiom equalInHead definedBy
	 ?x = ?y :- ?x[name hasValue ?n1] and ?y[name hasValue ?n2] and ?n1=?n2.










